								set {chest.%arg-2%.%arg-3%} to chestplate of arg-3
								set {legs.%arg-2%.%arg-3%} to leggings of arg-3
								set {boots.%arg-2%.%arg-3%} to boots of arg-3
								clear arg-3's helmet
								clear arg-3's chestplate
								clear arg-3's leggings
								clear arg-3's boots
								equip player argument with {helm.%arg-2%.%arg-3%}
								equip player argument with {chest.%arg-2%.%arg-3%} 
								equip player argument with {legs.%arg-2%.%arg-3%}
								equip player argument with {boots.%arg-2%.%arg-3%}
