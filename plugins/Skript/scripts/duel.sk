command /tparena <text> <integer>:
	permission: duel.arena.tp
	permission message: &cYou don't have permission for that.
	usage: &7/tparena &b<arena> <player> 
	trigger:
		if {arenas::%arg 1%} is set:
			if arg-2 = 1:
